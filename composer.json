{
    "name": "bean-component/php-thing",
    "type": "php-component",
    "description": "Provides a Component for Thing Domain Model.",
    "keywords": [ "php thing components" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Bean",
            "email": "peterbean@beancomponent.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "php": "^7.2.5",        
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*",
        "symfony/test-pack": "^1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/property-access": "^5.0",
        "dama/doctrine-test-bundle": "^6.2"        
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "Bean\\Thing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src-symfony/",
            "Bean\\Thing\\Doctrine\\": "doctrine/",
            "Bean\\Tests\\Thing\\": "tests/"
        }
    },
    "replace": {
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
